<% provide :title, (t ".suggests") %>
<h1><%= t ".suggests" %></h1>
<br/>
<table id="suggests-table", data-source="<%= admin_suggests_path(format: :json) %>">
  <thead>
    <tr>
      <th><%= t ".username" %></th>
      <th><%= t ".image" %></th>
      <th><%= t ".name" %></th>
      <th><%= t ".category" %></th>
      <th><%= t ".price" %></th>
      <th><%= t ".quantity" %></th>
      <th><%= t ".accept" %></th>
    </tr>
  </thead>
  <tbody>
  </tbody>
</table>
